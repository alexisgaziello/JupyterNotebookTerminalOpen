				<key>ActionParameters</key>
				<dict>
					<key>source</key>
					<string>on run {input, parameters}		(* Check if there is a file *)	set filesQty to count of input	if filesQty = 0 then		(* If no files, then ask which file to open *)		set pathFileAppleScript to choose file with prompt "Please select the jupyter notebook you wish to open:"	else		(* A file was open with the app *)		set pathFileAppleScript to item 1 of input	end if		(* Convert it to POSIX reference*)	set pathFilePOSIX to POSIX path of pathFileAppleScript		(* Add the '\' character before spaces*)	set AppleScript's text item delimiters to " "	set theTextItems to every text item of pathFilePOSIX	set AppleScript's text item delimiters to "\\ "	set pathFilePOSIX to theTextItems as string	set AppleScript's text item delimiters to ""		(* Execute jupyter notebook command in shell*)	tell application "Terminal"		activate		do script with command "jupyter notebook " &amp; pathFilePOSIX	end tell	end run</string>
